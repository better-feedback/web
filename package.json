{
  "name": "nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "asb": "echo 'start compiling contract'",
    "build:contract": "node contract/compile.js",
    "build:contract:debug": "node contract/compile.js --debug",
    "dev:deploy:contract": "near dev-deploy",
    "deploy:contract": "near deploy --accountId chezhe.testnet",
    "deploy": "npm run build && npm run deploy:contract && npm run deploy:pages",
    "prestart": "npm run build:contract:debug && npm run dev:deploy:contract"
  },
  "dependencies": {
    "bn.js": "^5.2.0",
    "dayjs": "^1.10.7",
    "grommet": "^2.19.0",
    "lodash": "^4.17.21",
    "near-api-js": "^0.43.1",
    "next": "12.0.4",
    "pubsub-js": "^1.9.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-feather": "^2.0.9",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.35",
    "eslint": "8.2.0",
    "eslint-config-next": "12.0.4",
    "shelljs": "^0.8.4",
    "typescript": "^4.5.2"
  }
}
